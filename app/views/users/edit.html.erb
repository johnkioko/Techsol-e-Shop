
<h1>Edit Your Account</h1>
<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  
  <%= f.label :password %>
  <%= f.password_field :password %>
  
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  
  <%= f.submit 'Update Account' %>
<% end %>
